#For Loop

years = c(1995, 1998,1984,1936,2005,1947)
for (yr in years) {
  print(yr)
}

for (yr in years) {
  if(yr < 1980){
    print('Old')
  }else{
    print('Not that old')
  }
  
}

#While Loop

count = 1
while (count <= 5) {
  print(c('Iteration number: ', count))
  count = count+1
  }


count = 1
while (count <= 4) {
